<launch>
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="robot_name"  default="robot1"/>
  
  <group ns = "$(arg robot_name)">
    <param name="odom"             value="/$(arg robot_name)/odom"/>

  </group>
  
  <!-- Launch node -->
  <node pkg="rotation_robot" type="gotogoals_innerlane1.py" name="gotogoals_innerlane" output="screen">
    
  </node>
</launch>
